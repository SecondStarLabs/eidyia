


<div class="row">
  <div class="col-sm-6">
    <h1>Cities</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%= link_to new_city_path, class: 'btn btn-primary' do %>
    Add New City
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    

            <th>City ascii</th>
            <th>State</th>
            <th>State name</th>
            <th>County fips</th>
            <th>County fips all</th>
            <th>County name all</th>
            <th>Lat</th>
            <th>Lng</th>
            <th>Population</th>
            <th>Density</th>
            <th>Source</th>
            <th>Military</th>
            <th>Incorporated</th>
            <th>Timezone</th>
            <th>Ranking</th>
            <th>Zips</th>
            
          <th></th>
        
      </tr>
    </thead>

    <tbody>
      <% @cities.each do |city| %>
        <%= content_tag :tr, id: dom_id(city), class: dom_class(city) do %>
          

                      <td><%= city.city_ascii %></td>
                      <td><%= city.state_id %></td>
                      <td><%= city.state_name %></td>
                      <td><%= city.county_fips %></td>
                      <td><%= city.county_fips_all %></td>
                      <td><%= city.county_name_all %></td>
                      <td><%= city.lat %></td>
                      <td><%= city.lng %></td>
                      <td><%= city.population %></td>
                      <td><%= city.density %></td>
                      <td><%= city.source %></td>
                      <td><%= city.military %></td>
                      <td><%= city.incorporated %></td>
                      <td><%= city.timezone %></td>
                      <td><%= city.ranking %></td>
                      <td><%= city.zips %></td>
          
          
            <td><%= link_to 'Show', city %></td>
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
